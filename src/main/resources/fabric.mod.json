{
    "schemaVersion": 1,
    "id": "${mod_id}",
    "version": "${mod_version}",
    "name": "${mod_name}",
    "description": "${mod_description}",
    "authors": [
        "ImToggle"
    ],
    "license": "LGPL-3.0-or-later",
    "environment": "*",
    "entrypoints": {
        "main": [
            {
                "adapter": "kotlin",
                "value": "me.imtoggle.notsoessentialplus.entrypoint.ModTemplateEntrypoint"
            }
        ]
    },
    "mixins": [
        "mixins.${mod_id}.json"
    ],
    "depends": {
        "minecraft": "~${minor_mc_version}",
        "fabric": "*",
        "fabricloader": ">=0.15.11",
        "fabric-language-kotlin": "*"
    }
}
